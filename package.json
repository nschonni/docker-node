{
  "name": "docker-node",
  "version": "0.0.1",
  "description": "[![dockeri.co](http://dockeri.co/image/_/node)](https://registry.hub.docker.com/_/node/)",
  "main": "update.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint-doctoc": "cp README.md README.md.tmp && doctoc --title='## Table of Contents' --github README.md && diff -q README.md README.md.tmp",
    "lint-eclint": "eclint check",
    "test": "npm run updateDockerfiles && npm run lint-eclint && npm run lint-doctoc",
    "updateDockerfiles": "node update.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nschonni/docker-node.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nschonni/docker-node/issues"
  },
  "homepage": "https://github.com/nschonni/docker-node#readme",
  "devDependencies": {
    "doctoc": "^1.3.1",
    "eclint": "^2.7.0",
    "glob": "^7.1.2",
    "js-yaml": "^3.12.0",
    "markdown-link-check": "^3.6.2",
    "request": "^2.87.0"
  }
}
